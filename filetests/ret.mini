fun foo(int i) int      # standard case
{
   return i;
}

#do control flow and make sure that function calls are matching arguments
fun main(int i) int      # control flow case
{
   if (i > 0)
   {
      return 1;
   }
   else
   {
      if (i < 0)
      {
         return -1;
      }
      else
      {
         return 0;
      }
   }
}


